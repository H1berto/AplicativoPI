<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.rickt.fam.Activity.Turma1Activity">

    <Button
        android:id="@+id/btnCadastrarNovoAluno"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_margin="16dp"
        android:text="CADASTRAR NOVO ALUNO"
        android:background="@drawable/shape_button_home"
        android:textColor="#fff"
        android:textStyle="bold"
        android:textSize="16sp"/>

    <android.support.v7.widget.RecyclerView
        android:id="@+id/reciclerView_aluno"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </android.support.v7.widget.RecyclerView>

</LinearLayout>
